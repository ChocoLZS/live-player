{
  "name": "my-next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:init": "wrangler d1 execute live-player --file=./schema.sql",
    "db:init:dev": "wrangler d1 execute live-player --file=./schema.sql --env=development --local",
    "db:seed": "tsx seed.ts"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.3.0",
    "artplayer": "^5.2.5",
    "bcryptjs": "^3.0.2",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.4",
    "hls.js": "^1.6.9",
    "jsonwebtoken": "^9.0.2",
    "next": "15.3.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250810.0",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.28.1"
  }
}
